{
  "name": "elder-scrolls-legends",
  "version": "0.1.0",
  "description": "Elder Scrolls Legends card explorer app",
  "main": "index.js",
  "author": "Vladimir Skok <skok@vova.io>",
  "license": "MIT",
  "keywords": [
    "react",
    "styled-components"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "lint:styles": "stylelint './src/**/*.js'",
    "format:styles": "stylelint --fix './src/**/*.js'",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,json}\"",
    "prepare": "yarn format && yarn format:styles && yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prepare"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "babel-plugin-styled-components": "^1.10.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "react-inlinesvg": "1.2.0",
    "stylelint": "13.3.3",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "svg-inline-loader": "0.8.2",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
